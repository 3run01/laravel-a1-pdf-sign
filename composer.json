	"minimum-stability": "dev",
	"require-dev": {
		"phpunit/phpunit": "^8.5.8|^9.3.3"
	},
	"authors": [
		{
			"name": "Lucas Nepomuceno",
			"email": "lsn.nepomuceno@gmail.com"
		}
